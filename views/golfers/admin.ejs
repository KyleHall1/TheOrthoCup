<h1>Golfer settings</h1>
<h2>Search Golfers</h2>
<form action="/golfers/admin" method="GET">
    <label>Name </label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% golfers.forEach(golfer => { %>
    <div><%= golfer.name %></div>
    <a href="/golfers/<%= golfer.id %>">View</a>
    <a href="/golfers/<%= golfer.id %>/edit">Edit</a>
    <form method="POST" action="/golfers/<%= golfer.id %>?_method=DELETE">
        <button type="submit">Delete</button>
    </form>
<% }) %>